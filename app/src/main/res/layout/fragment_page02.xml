<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="layout.Page02">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="#ffffff"
        android:orientation="vertical"
        android:paddingBottom="10dp">

        <TextView
            android:id="@+id/txtAddress2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="ที่อยู่"
            android:textStyle="bold"
            />
        <TextView
            android:id="@+id/txtAddress22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="20/1 ม.3 "
            android:layout_below="@+id/txtAddress2"
            />
        <TextView
            android:id="@+id/line011"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_below="@+id/txtAddress22"
            android:text="_____________________________________________ "
            />

        <!-- end of address-->

        <TextView
            android:id="@+id/txtTambon2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="ตำบล"
            android:textStyle="bold"
            android:layout_below="@id/line011"
            />
        <TextView
            android:id="@+id/txtTambon22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="สระแก้ว"
            android:layout_below="@id/txtTambon2"
            />

        <TextView
            android:id="@+id/txtAumper2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="อำเภอ"
            android:textStyle="bold"
            android:layout_marginLeft="50dp"
            android:layout_alignBaseline="@+id/txtTambon2"
            android:layout_alignBottom="@+id/txtTambon2"
            android:layout_toRightOf="@+id/txtAddress22"
            android:layout_toEndOf="@+id/txtAddress22"
            android:layout_marginStart="50dp" />

        <TextView
            android:id="@+id/txtAumper22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="เมือง"
            android:layout_alignBaseline="@+id/txtTambon22"
            android:layout_alignBottom="@+id/txtTambon22"
            android:layout_alignLeft="@+id/txtAumper2"
            android:layout_alignStart="@+id/txtAumper2" />
        <TextView
            android:id="@+id/line022"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_below="@id/txtTambon22"
            android:text="_____________________________________________ "
            />

        <!-- end of Tambon and Aumper -->

        <TextView
            android:id="@+id/txtProvince2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="จังหวัด"
            android:textStyle="bold"
            android:layout_below="@id/line022"
            />
        <TextView
            android:id="@+id/txtProvice22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="กำแพงเพชร"
            android:layout_below="@id/txtProvince2"
            />

        <TextView
            android:id="@+id/txtZipCode2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="รหัสไปรษณีย์"
            android:textStyle="bold"
            android:layout_marginLeft="50dp"
            android:layout_alignBaseline="@+id/txtProvince2"
            android:layout_alignBottom="@+id/txtProvice22"
            android:layout_toRightOf="@+id/txtAddress22"
            android:layout_toEndOf="@+id/txtAddress22"
            android:layout_marginStart="50dp" />

        <TextView
            android:id="@+id/txtZipCode22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="62000"
            android:layout_alignBaseline="@+id/txtProvice22"
            android:layout_alignBottom="@+id/txtTambon22"
            android:layout_alignLeft="@+id/txtAumper2"
            android:layout_alignStart="@+id/txtAumper2" />

        <TextView
            android:id="@+id/line03"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_below="@id/txtProvice22"
            android:text="_____________________________________________ "></TextView>

        <!-- end of Province and รหัสไปรษณีย์ -->
    </RelativeLayout>

</FrameLayout>
